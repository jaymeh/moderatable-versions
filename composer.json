{
    "name": "jaymeh/moderatable-versions",
    "description": "A new laravel package which combines versionable trait with approval",
    "license": "MIT",
    "authors": [
        {
            "name": "Jamie Sykes",
            "email": "contact@jaymeh.co.uk"
        }
    ],
    "require": {
        "php": ">=7.1.0 || >=7.2.5",
        "illuminate/support": "~5.3 || ^6.0 || ^7.0 || ^8.0",
        "mpociot/versionable": "^4.3"
    },
    "require-dev": {
        "phpunit/phpunit": "^7.0 || ^8.0 || ^9.0",
        "mockery/mockery": "^1.0",
        "orchestra/testbench": "^3.1 || ^4.0 || ^5.0 || ^6.0"
    },
    "autoload": {
        "classmap": [
            "src/migrations",
            "tests/ModeratableVersionsTestCase.php"
        ],
        "psr-0": {
            "Jaymeh\\ModeratableVersions": "src/"
        }
    },
    "scripts": {
        "test": "phpunit"
    },
    "extra": {
        "laravel": {
            "providers": [
                "Jaymeh\\ModeratableVersions\\Providers\\ServiceProvider"
            ]
        }
    }
}
